{
  "name": "DADbot",
  "version": "1.0.0",
  "description": "Discord Arduino Dashboard, or just DAD is a discord bot that alerts you of new member joins and more through use of a Arduino Uno",
  "license": "MIT",
  "author": "Kevin",
  "scripts": {
    "start": "node index.js",
    "update": "yarn update",
    "test": "eslint . silent"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/KCorb95/DADBot.git"
  },
  "bugs": {
    "url": "https://github.com/KCorb95/DADBot/issues"
  },
  "dependencies": {
    "chalk": "^2.4.2",
    "discord.js": "github:discordjs/discord.js.git#master",
    "johnny-five": "^1.0.0",
    "klasa": "github:dirigeants/klasa#master"
  },
  "devDependencies": {
    "eslint": "^5.16.0",
    "eslint-config-aqua": "^5.0.0"
  },
  "eslintConfig": {
    "extends": "aqua",
    "rules": {
      "es6": "true",
      "no-process-env": "off",
      "comma-dangle": "off",
      "quote-props": "off",
      "no-use-before-define": "off",
      "max-len": "off",
      "eol-last": "off",
      "no-console": "off",
      "no-inline-comments": "off",
      "capitalized-comments": "off",
      "no-undef": "off",
      "max-depth": "warn",
      "curly": "off",
      "nonblock-statement-body-position": "off",
      "indent": "off",
      "prefer-destructuring": [
        "warn"
      ]
    }
  },
  "engines": {
    "node": ">=11"
  }
}
